{
  "name": "get-order-stack-restaurant-api",
  "version": "1.0.0",
  "description": "GetOrderStack Restaurant API Platform",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "verify:marketplace:phase5": "tsx scripts/verify-marketplace-phase5.ts all",
    "verify:marketplace:phase5:contract": "tsx scripts/verify-marketplace-phase5.ts contract",
    "verify:marketplace:phase5:e2e": "tsx scripts/verify-marketplace-phase5.ts e2e",
    "pilot:marketplace:gates": "tsx scripts/marketplace-pilot-gate-report.ts",
    "migrate:delivery:profiles": "tsx scripts/migrate-delivery-provider-profiles.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed": "tsx scripts/seed-taipa.ts && tsx scripts/seed-demo-data.ts",
    "seed:auth": "tsx scripts/seed-auth.ts",
    "seed:reset": "prisma db push --force-reset --accept-data-loss && tsx scripts/seed-taipa.ts && tsx scripts/seed-demo-data.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@prisma/client": "^6.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "socket.io": "^4.7.4",
    "stripe": "^20.2.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.3",
    "prisma": "^6.0.0",
    "supertest": "^7.2.2",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "socket.io": {
      "engine.io": "^6.5.4"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
